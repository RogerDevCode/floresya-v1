{
  "yolo_mode": true,
  "auto_fix": true,
  "proactive": true,
  "behavior": {
    "ask_before_changes": false,
    "auto_correct_violations": true,
    "anticipate_needs": true,
    "verbose": false,
    "fail_fast": true
  },
  "rules": {
    "auto_fix_linting": true,
    "auto_add_try_catch": true,
    "auto_remove_fallbacks": true,
    "auto_organize_imports": true,
    "auto_remove_dead_code": true,
    "enforce_service_layer": true,
    "enforce_single_css": true
  },
  "response_format": {
    "concise": true,
    "action_first": true,
    "no_preamble": true,
    "use_checkmarks": true
  }
}