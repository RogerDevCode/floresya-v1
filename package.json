{
  "name": "floresya-v1",
  "version": "1.0.0",
  "description": "FloresYa - KISS E-commerce Platform for Flower Delivery",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run build:css && node api/server.js",
    "start": "node api/server.js",
    "build:icons": "node scripts/build-static-icons.js",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/tailwind.css --minify && npm run format",
    "build:css:optimized": "node build-css.js",
    "validate:icons": "node scripts/validate-static-icons.js",
    "watch:css": "tailwindcss -i ./public/css/input.css -o ./public/css/tailwind.css --watch",
    "copy:chart": "cp node_modules/chart.js/dist/chart.umd.min.js public/js/chart.min.js",
    "postinstall": "npm run copy:chart",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,css,json,md,html}\" --log-level=warn",
    "format:check": "prettier --check \"**/*.{js,css,json,md,html}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "docker compose exec cypress npx cypress run --config-file /cypress.config.js",
    "test:e2e:ci": "docker compose exec cypress npx cypress run --record --parallel --config-file /cypress.config.js",
    "test:e2e:standalone": "docker compose exec cypress npx cypress run --config-file /cypress.config.js",
    "test:e2e:ui": "docker compose exec cypress npx cypress open --config-file /cypress.config.js",
    "test:e2e:debug": "docker compose exec cypress npx cypress open --config-file /cypress.config.js",
    "test:e2e:docker": "docker compose up -d && docker compose exec cypress npx cypress run --config-file /cypress.config.js",
    "seed": "node seed-products.js",
    "seed:test": "node seed-test-data.js",
    "cleanup:test": "node cleanup-test-data.js",
    "serve": "serve public -p 3000",
    "generate:openapi": "node scripts/dev-tools/generate-openapi-spec.js",
    "generate:client": "node scripts/dev-tools/generate-api-client.js",
    "watch:openapi": "node scripts/watch-openapi.js",
    "validate:contract:ci": "node scripts/validation/validate-contract-ci.js",
    "validate:client:sync": "node scripts/validation/validate-client-spec-sync.js",
    "validate:frontend": "node scripts/validation/validate-frontend-contract.js",
    "validate:frontend:usage": "node scripts/validation/validate-frontend-usage.js",
    "validate:full": "npm run validate:contract:ci && npm run validate:client:sync && npm run validate:frontend && npm run validate:frontend:usage",
    "profile": "clinic doctor -- node api/server.js",
    "profile:cpu": "clinic flame -- node api/server.js",
    "profile:memory": "clinic heap-profiler -- node api/server.js",
    "profile:async": "clinic bubbleprof -- node api/server.js",
    "profile:auto": "node scripts/profiling/auto-profile.js",
    "profile:report": "node scripts/profiling/generate-report.js",
    "benchmark": "node scripts/benchmarking/benchmark-runner.js",
    "benchmark:ci": "node scripts/benchmarking/benchmark-runner.js --iterations 50 --concurrency 5",
    "code-review": "node scripts/code-review-checklist.js",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.2",
    "@supabase/supabase-js": "^2.48.0",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "glob": "^11.0.3",
    "helmet": "^8.0.0",
    "ioredis": "^5.8.2",
    "lucide": "^0.544.0",
    "lucide-react": "^0.548.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "redis": "^5.9.0",
    "sharp": "^0.34.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@clinic/bubbleprof": "^10.0.0",
    "@clinic/flame": "^13.0.0",
    "@clinic/heap-profiler": "^5.0.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/cli": "^4.1.13",
    "@testing-library/cypress": "^10.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "clinic": "^13.0.0",
    "cypress": "13.17.0",
    "eslint": "^9.17.0",
    "eslint-formatter-compact": "^9.0.1",
    "eslint-formatter-unix": "^8.40.0",
    "express-openapi-validator": "^5.6.0",
    "globals": "^15.14.0",
    "happy-dom": "^20.0.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "serve": "^14.2.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.13",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.1"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "ecommerce",
    "flowers",
    "vanilla-js",
    "supabase",
    "vercel",
    "kiss"
  ],
  "author": "FloresYa Team",
  "license": "MIT",
  "lint-staged": {
    "*.{js,css,json,md,html}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "main": "cleanup-test-products.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RogerDevCode/floresya-v1.git"
  },
  "bugs": {
    "url": "https://github.com/RogerDevCode/floresya-v1/issues"
  },
  "homepage": "https://github.com/RogerDevCode/floresya-v1#readme"
}
