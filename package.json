{
  "name": "floresya-v1",
  "version": "1.0.0",
  "description": "FloresYa - KISS E-commerce Platform for Flower Delivery",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "npm run build:css && node api/server.js",
    "start": "node api/server.js",
    "build:icons": "node scripts/build-static-icons.js",
    "build:css": "tailwindcss -i ./public/css/input.css -o ./public/css/tailwind.css --minify && npm run format",
    "build:css:optimized": "node build-css.js",
    "validate:icons": "node scripts/validate-static-icons.js",
    "watch:css": "tailwindcss -i ./public/css/input.css -o ./public/css/tailwind.css --watch",
    "copy:chart": "cp node_modules/chart.js/dist/chart.umd.min.js public/js/chart.min.js",
    "postinstall": "npm run copy:chart",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,css,json,md,html}\" --log-level=warn",
    "format:check": "prettier --check \"**/*.{js,css,json,md,html}\"",
    "test": "NODE_OPTIONS='--max-old-space-size=512' vitest run",
    "test:watch": "NODE_OPTIONS='--max-old-space-size=512' vitest",
    "test:unit": "NODE_OPTIONS='--max-old-space-size=512' vitest run",
    "test:integration": "NODE_OPTIONS='--max-old-space-size=512' vitest run test/integration",
    "test:ui": "NODE_OPTIONS='--max-old-space-size=512' vitest --ui",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=512' vitest run --coverage --maxConcurrency=2",
    "test:e2e": "NODE_OPTIONS='--max-old-space-size=512' playwright test",
    "test:e2e:headed": "NODE_OPTIONS='--max-old-space-size=512' playwright test --headed",
    "test:e2e:ui": "NODE_OPTIONS='--max-old-space-size=512' playwright test --ui",
    "test:e2e:debug": "NODE_OPTIONS='--max-old-space-size=512' playwright test --debug",
    "test:e2e:report": "NODE_OPTIONS='--max-old-space-size=512' npx playwright show-report",
    "seed": "node seed-products.js",
    "seed:test": "node seed-test-data.js",
    "cleanup:test": "node cleanup-test-data.js",
    "serve": "serve public -p 3000",
    "generate:openapi": "node scripts/dev-tools/generate-openapi-spec.js",
    "generate:client": "node scripts/dev-tools/generate-api-client.js",
    "watch:openapi": "node scripts/watch-openapi.js",
    "validate:contract:ci": "node scripts/validation/validate-contract-ci.js",
    "validate:client:sync": "node scripts/validation/validate-client-spec-sync.js",
    "validate:frontend": "node scripts/validation/validate-frontend-contract.js",
    "validate:frontend:usage": "node scripts/validation/validate-frontend-usage.js",
    "validate:full": "npm run validate:contract:ci && npm run validate:client:sync && npm run validate:frontend && npm run validate:frontend:usage",
    "profile": "clinic doctor -- node api/server.js",
    "profile:cpu": "clinic flame -- node api/server.js",
    "profile:memory": "clinic heap-profiler -- node api/server.js",
    "profile:async": "clinic bubbleprof -- node api/server.js",
    "profile:auto": "node scripts/profiling/auto-profile.js",
    "profile:report": "node scripts/profiling/generate-report.js",
    "benchmark": "node scripts/benchmarking/benchmark-runner.js",
    "benchmark:ci": "node scripts/benchmarking/benchmark-runner.js --iterations 50 --concurrency 5",
    "mutation": "bash scripts/run-mutation-tests.sh",
    "mutation:incremental": "bash scripts/run-mutation-tests.sh --incremental",
    "mutation:services": "bash scripts/run-mutation-tests.sh --mutate 'api/services/**/*.js'",
    "mutation:repositories": "bash scripts/run-mutation-tests.sh --mutate 'api/repositories/**/*.js'",
    "mutation:middleware": "bash scripts/run-mutation-tests.sh --mutate 'api/middleware/**/*.js'",
    "code-review": "node scripts/code-review-checklist.js",
    "prepare": "husky || true",
    "serve:static": "npx http-server public -p 3000 -c-1"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@supabase/supabase-js": "^2.84.0",
    "chart.js": "^4.5.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-openapi-validator": "^5.6.0",
    "express-session": "^1.18.2",
    "form-data": "^4.0.5",
    "glob": "^13.0.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "lucide": "^0.554.0",
    "lucide-react": "^0.554.0",
    "multer": "^2.0.2",
    "pg": "^8.16.3",
    "redis": "^5.10.0",
    "sharp": "^0.34.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.18.3",
    "xss-clean": "^0.1.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.11.0",
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@stryker-mutator/core": "^9.3.0",
    "@stryker-mutator/javascript-mutator": "^4.0.0",
    "@stryker-mutator/jest-runner": "^9.3.0",
    "@tailwindcss/cli": "^4.1.17",
    "@vitest/coverage-v8": "^4.0.13",
    "@vitest/ui": "^4.0.13",
    "acorn": "^8.15.0",
    "acorn-walk": "^8.3.4",
    "autoprefixer": "^10.4.22",
    "chokidar": "^4.0.3",
    "eslint": "^9.39.1",
    "eslint-formatter-compact": "^9.0.1",
    "eslint-formatter-unix": "^9.0.1",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "serve": "^14.2.5",
    "supertest": "^7.1.4",
    "tailwindcss": "^4.1.17",
    "vitest": "^4.0.13",
    "wait-on": "^9.0.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "ecommerce",
    "flowers",
    "vanilla-js",
    "supabase",
    "vercel",
    "kiss"
  ],
  "author": "FloresYa Team",
  "license": "MIT",
  "lint-staged": {
    "*.{js,css,json,md,html}": [
      "prettier --write"
    ],
    "*.js": [
      "eslint --fix"
    ]
  },
  "main": "cleanup-test-products.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RogerDevCode/floresya-v1.git"
  },
  "bugs": {
    "url": "https://github.com/RogerDevCode/floresya-v1/issues"
  },
  "homepage": "https://github.com/RogerDevCode/floresya-v1#readme"
}
